{
    "eslint.validate": [
        "javascript",
        "typescript"
    ],
    "stylelint.validate": [
        "css"
    ],
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true,
        "source.fixAll.stylelint": true
    },
    "cSpell.words": [
        "figma",
        "retina",
        "stylelint",
        "oldstyle",
        "nums",
        "ldquo",
        "rdquo",
        "debuff"
    ]
}